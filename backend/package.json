    "@tensorflow/tfjs-node": "^4.15.0",
    "@xenova/transformers": "^2.12.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.15.4",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "compromise": "^14.10.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "fast-csv": "^5.0.1",
    "franc": "^6.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "isomorphic-dompurify": "^2.6.0",
    "jsonwebtoken": "^9.0.2",
    "keyword-extractor": "^0.0.28",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "natural": "^6.12.0",
    "node-cron": "^4.2.1",
    "openai": "^4.26.0",
    "pdfjs-dist": "^4.0.379",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "sentiment": "^5.0.2",
    "socket.io": "^4.7.4",
    "stopword": "^2.0.8",
